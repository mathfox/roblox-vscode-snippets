{
    "require script-related ModuleScript": {
        "prefix": [
            "sreq"
        ],
        "body": [
            "local $1 = require(script.$1)$0"
        ]
    },
    "insert Roblox Service": {
        "prefix": [
            "service"
        ],
        "body": [
            "local $1 = game:GetService(\"$1\")$0"
        ]
    },
    "insert Unite service": {
        "prefix": [
            "uniteService"
        ],
        "body": [
            "local $1 = Unite.getService(\"$1\")$0"
        ]
    },
    "insert Unite controller": {
        "prefix": [
            "uniteController"
        ],
        "body": [
            "local $1 = Unite.getController(\"$1\")$0"
        ]
    },
    "insert Unite framework": {
        "prefix": [
            "unite"
        ],
        "body": [
            "local Unite = require(game:GetService(\"ReplicatedStorage\").Unite)"
        ]
    },
    "insert Shared util": {
        "prefix": [
            "utilShared"
        ],
        "body": [
            "local $1 = Unite.getSharedUtil(\"$1\")$0"
        ]
    },
    "insert Server util": {
        "prefix": [
            "utilServer"
        ],
        "body": [
            "local $1 = Unite.getServerUtil(\"$1\")$0"
        ]
    },
    "insert Client util": {
        "prefix": [
            "utilClient"
        ],
        "body": [
            "local $1 = Unite.getClientUtil(\"$1\")$0"
        ]
    },
    "insert Shared module": {
        "prefix": [
            "moduleShared"
        ],
        "body": [
            "local $1 = Unite.getSharedModule(\"$1\")$0"
        ]
    },
    "insert Server Module": {
        "prefix": [
            "moduleServer"
        ],
        "body": [
            "local $1 = Unite.getServerModule(\"$1\")$0"
        ]
    },
    "insert Client module": {
        "prefix": [
            "moduleClient"
        ],
        "body": [
            "local $1 = Unite.getClientModule(\"$1\")$0"
        ]
    },
    "insert UDim2.fromScale": {
        "prefix": [
            "scale"
        ],
        "body": [
            "UDim2.fromScale(0$1, 0$2)${3:,}$0"
        ]
    },
    "insert Roact": {
        "prefix": "roact",
        "body": [
            "local Unite = require(game:GetService(\"ReplicatedStorage\").Unite)",
            "",
            "local Roact = Unite.getSharedUtil($\"Roact\")"
        ],
        "description": "insert Roact"
    },
    "insert Roact element": {
        "prefix": [
            "element"
        ],
        "body": [
            "Roact.createElement($0),"
        ]
    },
    "insert Roact element without comma": {
        "prefix": [
            "relement"
        ],
        "body": [
            "Roact.createElement($0)"
        ]
    },
    "create Roact ref": {
        "prefix": [
            "createRef"
        ],
        "body": [
            "Roact.createRef()${1:,}$0"
        ]
    },
    "insert UIAspectRatioConstraint": {
        "prefix": [
            "ratio"
        ],
        "body": [
            "Roact.createElement(\"UIAspectRatioConstraint\"$0),"
        ],
        "description": "A shorthand to insert UIAspectRatioConstraint using Roact"
    },
    "assign Roact.Ref": {
        "prefix": [
            "ref"
        ],
        "body": [
            "[Roact.Ref] = self.$0"
        ]
    },
    "assign Roact.Event": {
        "prefix": [
            "event"
        ],
        "body": [
            "[Roact.Event.${1|Activated,InputBegan,InputChanged,InputEnded,MouseButton1Click,MouseButton1Down,MouseButton1Up,MouseButton2Click,MouseButton2Down,MouseButton2Up|}] = $0"
        ]
    },
    "Roact Component render method": {
        "prefix": [
            "render"
        ],
        "body": [
            "function ${1:$TM_FILENAME_BASE}:render()",
            "\treturn $0",
            "end"
        ]
    },
    "Roact init method": {
        "prefix": [
            "init"
        ],
        "body": [
            "function ${1:$TM_FILENAME_BASE}:init()",
            "\t$0",
            "end"
        ]
    },
    "Roact Component didMount method": {
        "prefix": [
            "didMount"
        ],
        "body": [
            "function ${1:$TM_FILENAME_BASE}:didMount()",
            "\t$0",
            "end"
        ]
    },
    "Roact Component": {
        "prefix": [
            "rc"
        ],
        "body": [
            "local ${1:$TM_FILENAME_BASE} = Roact.Component:extend(\"${1:$TM_FILENAME_BASE}\")\n",
            "function ${1:$TM_FILENAME_BASE}:render()",
            "\treturn $0",
            "end\n",
            "return ${1:$TM_FILENAME_BASE}"
        ]
    },
    "Roact-Rodux Component": {
        "prefix": [
            "rdc"
        ],
        "body": [
            "local ${1:$TM_FILENAME_BASE} = Roact.Component:extend(\"${1:$TM_FILENAME_BASE}\")\n",
            "function ${1:$TM_FILENAME_BASE}:render()",
            "\treturn $0",
            "end\n",
            "local function mapStateToProps(storeState, props)",
            "\treturn {}",
            "end\n",
            "local function mapDispatchToProps(dispatch)",
            "\treturn {}",
            "end\n",
            "return RoactRodux.connect(mapStateToProps, mapDispatchToProps)(${1:$TM_FILENAME_BASE})"
        ]
    }
}